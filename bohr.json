{
  "name": "customer-success",
  "functions": {
    "api": {
      "runtime": "nodejs18.x",
      "entry": "api/core/index.js"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ],
  "build": {
    "command": "npm run build",
    "output": ".next"
  }
}